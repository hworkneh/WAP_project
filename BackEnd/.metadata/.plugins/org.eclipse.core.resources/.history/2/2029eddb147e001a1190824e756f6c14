		this.db=db;
		
	}
	
	public List<Account> findAll(){
		Account result=null;
		ResultSet rs;
		try {
			rs = db.runSql("select * from account");
			int i=0;
			while(rs.next()) {
				System.out.println(rs.getString(i));
				i++;
			}
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return null;
	}
	
